{
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "off"
  },
  "breadcrumbs.enabled": true,
  "debug.inlineValues": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontFamily": "WenQuanYi Mono for Powerline",
  "editor.minimap.enabled": false,
  "editor.minimap.renderCharacters": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 100, 120],
  "editor.wordWrap": "off",
  "explorer.openEditors.dynamicHeight": false,
  "explorer.openEditors.visible": 5,
  "git.autofetch": true,
  "gitlens.codeLens.enabled": false,
  "macros": {
    "autoFix": ["eslint.executeAutofix", "tslint.fixAllProblems"]
  },
  "markdown.extension.orderedList.autoRenumber": false,
  "markdown.extension.tableFormatter.enabled": false,
  "markdown.extension.toc.updateOnSave": false,
  /* https://github.com/Microsoft/vscode/issues/36987#issuecomment-340147201 */
  "terminal.integrated.shellArgs.osx": [],
  "todohighlight.defaultStyle": {
    "backgroundColor": "rgba(0,0,0,0)",
    "border": "1px solid red",
    "borderRadius": "2px",
    "color": "red"
  },
  "todohighlight.keywordsPattern": "(TODO|FIXME|NOTE)(\\(.*?\\))?:?",
  "tslint.jsEnable": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.nativeTabs": true,
  "workbench.colorCustomizations": {
    "[One Dark Pro]": {
      /* https://github.com/Binaryify/OneDark-Pro/issues/231 */
      "activityBar.background": "#2F333D",
      "editorWhitespace.foreground": "#666666",
      "gitDecoration.ignoredResourceForeground": "#888888",
      /* https://github.com/Tyriar/vscode-snazzy/blob/master/snazzy.json */
      "terminal.ansiBlack": "#282a36",
      "terminal.ansiBlue": "#57c7ff",
      "terminal.ansiBrightBlack": "#686868",
      "terminal.ansiBrightBlue": "#57c7ff",
      "terminal.ansiBrightCyan": "#9aedfe",
      "terminal.ansiBrightGreen": "#5af78e",
      "terminal.ansiBrightMagenta": "#ff6ac1",
      "terminal.ansiBrightRed": "#ff5c57",
      "terminal.ansiBrightWhite": "#eff0eb",
      "terminal.ansiBrightYellow": "#f3f99d",
      "terminal.ansiCyan": "#9aedfe",
      "terminal.ansiGreen": "#5af78e",
      "terminal.ansiMagenta": "#ff6ac1",
      "terminal.ansiRed": "#ff5c57",
      "terminal.ansiWhite": "#f1f1f0",
      "terminal.ansiYellow": "#f3f99d",
      "terminal.background": "#282a36",
      "terminal.foreground": "#eff0eb"
    }
  },
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "newUntitledFile"
}
