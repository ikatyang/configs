[
  {
    "key": "alt+space",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+f",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+s",
    "command": "tslint.fixAllProblems",
    "when": "editorLangId == 'typescript' && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+s",
    "command": "tslint.fixAllProblems",
    "when": "editorLangId == 'typescriptreact' && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+s",
    "command": "eslint.executeAutofix",
    "when": "editorLangId == 'javascript' && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+s",
    "command": "eslint.executeAutofix",
    "when": "editorLangId == 'javascriptreact' && editorTextFocus && !editorReadonly"
  }
]
